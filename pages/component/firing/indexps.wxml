<view class="firingcontent">
    <image alt class="firingimglogo _img" mode="widthFix" src="https://diag.wuzhengai.com/appimage/logo7_4.png"></image>
    <view class="firingappname">{{name}}</view>
    <button bindgetphonenumber="__e" class="btnzhuce login-but" data-event-opts="{{[ [ 'getphonenumber',[ [ 'getPhoneNumber',['$event'] ] ] ] ]}}" openType="getPhoneNumber">手机号快捷登录</button>
    <button bindtap="__e" class="btnzhuceps" data-event-opts="{{[ [ 'tap',[ ['tiaops'] ] ] ]}}">其他方式登录</button>
    <view bindtap="__e" class="btnzhuceps1" data-event-opts="{{[ [ 'tap',[ ['budeng'] ] ] ]}}">暂不登陆</view>
    <view class="btnzhucexieyi">
        <image alt bindtap="__e" class="btnzhucexieyiimg _img" data-event-opts="{{[ [ 'tap',[ [ 'xieyi',['$event'] ] ] ] ]}}" src="https://diag.wuzhengai.com/image/xieyi1.png" wx:if="{{radioin}}"></image>
        <image alt bindtap="__e" class="btnzhucexieyiimg _img" data-event-opts="{{[ [ 'tap',[ [ 'xieyi',['$event'] ] ] ] ]}}" src="https://diag.wuzhengai.com/image/xieyi.png" wx:if="{{!radioin}}"></image>
        <view class="btnzhucexieyitext">
            <text>已阅读并同意</text>
            <text bindtap="__e" class="btnzhucexieyitextps" data-event-opts="{{[ [ 'tap',[ [ 'xieyiin',[0] ] ] ] ]}}">《用户协议》</text>、<text bindtap="__e" class="btnzhucexieyitextps" data-event-opts="{{[ [ 'tap',[ [ 'xieyiin',[1] ] ] ] ]}}">《隐私协议》</text>
        </view>
    </view>
</view>
